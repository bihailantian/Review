<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center_horizontal"
    tools:context=".activity.VectorDrawableToImageActivity">

    <ImageView
        android:id="@+id/ic_baseline_check_24"
        android:layout_marginTop="16dp"
        android:src="@drawable/ic_baseline_check_24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <ImageView
        android:id="@+id/ic_no_pass"
        android:layout_marginTop="16dp"
        android:src="@drawable/ic_no_pass"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <ImageView
        android:id="@+id/ic_not_happen"
        android:layout_marginTop="16dp"
        android:src="@drawable/ic_not_happen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />


    <Button
        android:layout_margin="16dp"
        android:textAllCaps="false"
        android:text="vector to image"
        android:id="@+id/btn_vector2Img"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</LinearLayout>